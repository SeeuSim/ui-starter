{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Express",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "strict": true ,
    "skipLibCheck": true
  },
  "ts-node": {
    "swc": true,
    "require": ["tsconfig-paths/register"]
  }
}
